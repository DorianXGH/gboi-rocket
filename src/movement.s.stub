; //////// MOVE ISAAC \\\\\\\
	ld a,(global_.isaac.speed)
	and %00001111
	bit $3,a
	jp z,@dx_pos
	or %11110000
@dx_pos:
	ld hl,global_.isaac.y
	add (hl)
	ld (hl),a

	ld a,(global_.isaac.speed)
	rrca
	rrca
	rrca
	rrca
	and %00001111
	bit $3,a
	jp z,@dy_pos
	or %11110000
@dy_pos:
	ld hl,global_.isaac.x
	add (hl)
	ld (hl),a
; \\\\\\\ MOVE ISAAC ////////
